<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="lelPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Conceptcategory</class>
    <class>model.Documentdef</class>
    <class>model.Conceptlog</class>
    <class>model.Conceptdetails</class>
    <class>model.Conceptclass</class>
    <class>model.Conceptactiondef</class>
    <class>model.Userlog</class>
    <class>model.Conceptclassdef</class>
    <class>model.Useractiondef</class>
    <class>model.Concept</class>
    <class>model.User</class>
    <class>model.Conceptcategorydef</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/lel"/>
      <property name="javax.persistence.jdbc.password" value="mysqlpass"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
